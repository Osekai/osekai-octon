@using Osekai.Octon
@model Osekai.Octon.Pages.AppBaseLayout

@{
    Layout = "BaseLayout";
}

@section HeadExtension {
    <link rel="stylesheet" href=@PathUtils.AppendVersionQueryCached($"/static/{Model.App.SimpleName}/css/main.css") />
    @if (Model.App.Experimental) {<link rel="stylesheet" href=@PathUtils.AppendVersionQueryCached($"/static/shared/css/experimental/main.css") />}
}

@RenderBody()